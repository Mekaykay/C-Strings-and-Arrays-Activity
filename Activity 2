#include <stdio.h>
#include <string.h>
int main()
{
   char string1[50], string2[50], i, j;
   printf("\nEnter string1: ");
   scanf("%s",string1);
   printf("\nEnter string2: ");
   scanf("%s",string2);

   for(i=0; string1[i]!='\0'; ++i);


   for(j=0; string2[j]!='\0'; ++j, ++i)
   {
      string1[i]=string2[j];
   }
   string1[i]='\0';
   printf("\nOutput: %s",str1);

   return 0;
}
